<div class="normalheader transition animated fadeIn">
    <div class="hpanel">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6">
                    <h2 class="font-light m-b-xs">
                        Location List: <%= (cities[req.params.id].name != undefined) ? cities[req.params.id].name : "" %>
                    </h2>
                    <div id="hbreadcrumb">
                        <ol class="hbreadcrumb breadcrumb">
                            <li><a title="Dashboard" href="<%= sails.config.base_url %>dashboard">Dashboard</a></li>
                            <li class="active">
                                <span>Location List</span>
                            </li>
                        </ol>
                    </div>
                </div>
                <div class="col-md-6 text-right m-t-sm">
                  <a class="btn btn-primary" href="<%= sails.config.base_url %>city/addCity/0"><i class="icon icon-user"></i>Add Location</a>
                </div>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="city_id" name="city_id" value="<%= (cityId != undefined) ? cityId : '' %>">
<div class="hpanel">
  <div class="panel-body">
    <table  id="location-grid"></table>
    <div id="location-grid-pager"></div>

  </div>
</div>
