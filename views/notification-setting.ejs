<div class="normalheader transition animated fadeIn">
  <div class="hpanel">
    <div class="panel-body">
      <div class="row">
        <div class="col-md-6">
          <h2 class="font-light m-b-xs">
            Notification Setting
          </h2>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="alert alert-success" style="display: none"></div>
<div class="alert alert-danger" style="display: none"></div>
<div class="row">
  <div class="col-md-8">
    <div class="hpanel">
      <div class="panel-body">
        <div class="notification-section">
          <ul class="list-group">
            <li class="list-group-item">
            <span class="badge">
            <div class="btn-group btn-toggle change-notification is-user-notification">
              <button class="btn btn-lg <%= (userDetail.is_user_notification_enable == 'true' || userDetail.is_user_notification_enable == true) ? "btn-primary active" : "btn-default" %>" data-value="true">Yes</button>
              <button class="btn btn-lg <%= (userDetail.is_user_notification_enable == 'false' || userDetail.is_user_notification_enable == false) ? "btn-primary active" : "btn-default" %>" data-value="false">No</button>
            </div>
            </span>
              <h6>Notification for Users</h6>
            </li>
            <li class="list-group-item">
            <span class="badge">
            <div class="btn-group btn-toggle change-notification is-device-notification">
              <button class="btn btn-lg <%= (userDetail.is_device_notification_enable == true || userDetail.is_device_notification_enable == 'true') ? 'btn-primary active' : 'btn-default' %>" data-value="true">Yes</button>
              <button class="btn btn-lg <%= (userDetail.is_device_notification_enable == false || userDetail.is_device_notification_enable == 'false') ? 'btn-primary active' : 'btn-default' %>" data-value="false">No</button>
            </div>
            </span><h6> Notification for Devices</h6>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
