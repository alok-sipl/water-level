<%- body %>
<!-- Vendor scripts -->
<script src="<%= sails.config.base_url %>custom/js/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="<%= sails.config.base_url %>custom/js/vendor/metisMenu/dist/metisMenu.min.js"></script>
<script src="<%= sails.config.base_url %>custom/js/vendor/parsley.js"></script>
<script src="<%= sails.config.base_url %>custom/js/homer.js"></script>
<script src="<%= sails.config.base_url %>custom/js/login.js"></script>
</body>
</html>
