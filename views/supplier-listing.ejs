<div class="normalheader transition animated fadeIn">
    <div class="hpanel">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-6">
                    <h2 class="font-light m-b-xs">
                        Supplier List
                    </h2>
                    <div id="hbreadcrumb">
                        <ol class="hbreadcrumb breadcrumb">
                            <li><a title="Dashboard" href="<%= sails.config.base_url %>dashboard">Dashboard</a></li>
                            <li class="active">
                                <span>Supplier List</span>
                            </li>
                        </ol>
                    </div>
                </div>
                <div class="col-md-6 text-right m-t-sm">
                    <a class="btn btn-primary" href="<%= sails.config.base_url %>supplier/addSupplier">Add Supplier</a>
                </div>
            </div>

        </div>
    </div>
</div>
<div class="hpanel">
    <div class="panel-body">
      <% if (req.session.flash) { %>
      <%- req.flash('flashMessage') %>
      <% } %>
        <table class="table table-striped table-bordered table-hover dataTable no-footer" id="user-table">
            <thead>
                <tr>
                    <th class="text-left">Name</th>
                    <th class="text-left">Email</th>
                    <th class="text-left">Contact No.</th>
                    <th class="text-left">Location</th>
                    <th class="text-left">City</th>
                    <th class="th-status">Status</th>
                    <th class="th-action">Action</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
