<!-- Header -->
<div id="header">
  <div id="logo" class="light-version">
      <a href="<%= sails.config.base_url %>" ><img src="<%= sails.config.base_url %>images/logo.png" title="<%= sails.config.app_name %>" alt="<%= sails.config.app_name %>"></a>
  </div>
  <nav role="navigation">
    <!--<div class="header-link hide-menu"><i class="fa fa-bars"></i></div>-->
    <div class="small-logo">
      <span class="text-primary"><%= sails.config.app_name %></span>
    </div>
    <div class="navbar-right">
      <ul class="nav navbar-nav no-borders col-md-12">
          <li class="<%= (req.options.controller == 'supplier') ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>supplier" title="Manage Supplier"><span
              class="nav-label">Manage Supplier</span></a>
        </li>
        <li class="<%= (req.options.controller == 'user') ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>user" title="Manage User"><span class="nav-label">Manage User</span></a>
        </li>
        <li class="<%= (req.options.controller == 'device') ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>device" title="Manage Device"><span class="nav-label">Manage Device</span></a>
        </li>
        <li class="<%= (req.options.controller == 'city') ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>city" title="Manage City"><span class="nav-label">Manage City</span></a>
        </li>
        <li class="<%= (req.options.controller == 'contact') ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>contact" title="View Enquiry"><span class="nav-label">View Enquiry</span></a>
        </li>
        <li class="<%= ((req.options.controller == 'dashboard') &&  (req.options.action == 'setting')) ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>dashboard/setting" title="Setting"><span class="nav-label">Setting</span></a>
        </li>
        <li class="<%= ((req.options.controller == 'dashboard') &&  (req.options.action == 'profile')) ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>dashboard/profile" title="Profile"><span class="nav-label">Profile</span></a>
        </li>
        <li class="<%= ((req.options.controller == 'dashboard') &&  (req.options.action == 'changePassword')) ? 'active' : ''%>">
          <a href="<%= sails.config.base_url %>dashboard/changePassword" title="Change Password"><span class="nav-label">Change Password</span></a>
        </li>
        <li class="dropdown">
            <a title="Alerts" class="dropdown-toggle label-menu-corner logout" href="#" data-toggle="dropdown" aria-expanded="false">
                <i class="pe-7s-bell"></i>
                <span class="label label-success">4</span>
            </a>
            <ul class="dropdown-menu hdropdown animated flipInX">
                <div class="title">
                    You have 4 new messages
                </div>
                <li>
                    <a>
                        It is a long established.
                    </a>
                </li>
                <li>
                    <a>
                        There are many variations.
                    </a>
                </li>
                <li>
                    <a>
                        Lorem Ipsum is simply dummy.
                    </a>
                </li>
                <li>
                    <a>
                        Contrary to popular belief.
                    </a>
                </li>
                <li class="summary"><a href="#">See All Messages</a></li>
            </ul>
        </li>
        <li>
          <a title="Logout" href="<%= sails.config.base_url %>dashboard/logout" logout class="logout">
            <i class="pe-7s-upload pe-rotate-90"></i>
          </a>
        </li>
      </ul>
    </div>
  </nav>
</div>
<!-- Main Wrapper -->
<div id="wrapper">
  <div class="content animate-panel">
    <div class="row">
      <div class="col-lg-12">
        <%- body %>
      </div>
      <p class="footer text-center m-b-none">
        Â© <%= (new Date()).getFullYear() %> <span class="m-b-none"><%= sails.config.app_name %></span> All Rights Reserved.
      </p>

    </div>
  </div>
</div>
<div class="modal iframe-modal-wrapper" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-lg">
    <div class="modal-content">
      <div class="modal-header">
        <div class="row">
          <div class="col-md-10">
            <h3 class="modal-title m-t-0">VIEW</h3>
          </div>
          <div class="col-md-2 text-right">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true"><i
                  class="fa fa-times" aria-hidden="true"></i></span></button>
          </div>
        </div>
      </div>
      <div class="modal-body">
        <iframe src="" class="iframe bg-white" frameborder="0" name="info" seamless height="100%" width="100%"></iframe>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->
