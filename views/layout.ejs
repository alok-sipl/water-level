<!-- This is the Javascript file of jqGrid -->
<script type="text/ecmascript"
        src="https://cdnjs.cloudflare.com/ajax/libs/free-jqgrid/4.12.0/js/jquery.jqgrid.min.js"></script>
</body>
</html>
